            <Parallel DisplayName="编译、测试并关联变更集和工作项">
              <TryCatch DisplayName="尝试编译和测试" mtbwt:BuildTrackingParticipant.Importance="Low">
                <TryCatch.Try>
                  <Sequence DisplayName="编译和测试">
                    <Sequence mtbwt:BuildTrackingParticipant.Importance="None">
